{
    "name": "clicktrend/frontend-validation-bundle",
    "version": "0.1",
    "description": "jQuery frontend validation as symfony2 bundle",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Kadir Yuecel",
            "email": "ky@clicktrend.de"
        }
    ],
    "type": "symfony-bundle",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "reactive-raven/jq-bootstrap-validation",
                "version": "1.3.7",
                "dist": {
                    "url": "https://github.com/ReactiveRaven/jqBootstrapValidation/archive/1.3.7.zip",
                    "type": "zip"
                },
                "packagist": false
            }
        }
    ],
    "require": {
        "symfony/framework-bundle": ">=2.0,<2.4-dev",
        "reactive-raven/jq-bootstrap-validation": "1.3.7",
        "braincrafted/bootstrap-bundle": "dev-master",
        "twitter/bootstrap": "3.0.2"
    },
    "autoload": {
        "psr-0": { "Clicktrend\\Bundle\\FrontendValidationBundle": "" }
    },
    "target-dir": "Clicktrend/Bundle/FrontendValidationBundle",
    "minimum-stability": "dev"
}